{
    "name": "shawnhooper/outdated-to-jira",
    "description": "Creates JIRA tickets for outdated Composer and npm dependencies.",
    "type": "project",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.0",
        "symfony/console": "^6.0|^7.0",
        "vlucas/phpdotenv": "^5.4",
        "psr/log": "^1.1 || ^2.0 || ^3.0",
        "symfony/process": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Shawn Hooper",
            "email": "shawn@shawnhooper.ca"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^12.1",
        "squizlabs/php_codesniffer": "^3.12",
        "phpstan/phpstan": "^2.1"
    }
}
